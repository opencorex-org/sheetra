{
  "name": "sheetra",
  "version": "1.0.2",
  "description": "Powerful Excel/CSV/JSON export library with zero dependencies",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:performance": "jest test/performance --runInBand",
    "test:core": "jest test/unit/core",
    "test:builders": "jest test/unit/builders",
    "test:writers": "jest test/unit/writers",
    "test:formatters": "jest test/unit/formatters",
    "test:utils": "jest test/unit/utils",
    "test:changed": "jest --onlyChanged",
    "test:related": "jest --findRelatedTests",
    "test:verbose": "jest --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:clear": "jest --clearCache",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:coverage:html": "jest --coverage && open coverage/index.html",
    "test:ci:coverage": "jest --ci --coverage --coverageReporters=text-lcov | coveralls",
    "test:badges": "jest --coverage && jest-coverage-badges output ./coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "excel",
    "csv",
    "json",
    "export",
    "spreadsheet",
    "xlsx",
    "data-export",
    "zero-dependencies"
  ],
  "author": "Your Name",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/opencorex-org/sheetra.git"
  },
  "bugs": {
    "url": "https://github.com/opencorex-org/sheetra/issues"
  },
  "homepage": "https://github.com/opencorex-org/sheetra#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@jest/types": "^30.2.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.0.0",
    "typescript": "^5.0.0"
  }
}